
<h2>Students</h2>
 <table>
     <tr>
     <th>Grade</th>
     <th>Fistname</th>
     <th>Lastname</th>
    </tr>
    <% students.forEach(function(student){ %>
    <tr>
         <td><%= student.grade%></td>
        <td><%= student.firstname%></td>
        <td><%= student.lastname%></td>
    </tr>
    <%  });%>
 </table>


